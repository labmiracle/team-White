{
    "files.exclude": {
        "**/build": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/coverage": true,
        "package-lock.json": true
    },

    "workbench.colorCustomizations": {
        // a unique titlebar color scheme helps differentiate project windows (eg: Windows taskbar hover preview)
        "activityBar.background": "#164272",
        "titleBar.activeBackground": "#102540",
        "titleBar.activeForeground": "#FEFCFE"
    },
    "cSpell.words": ["cacheable", "devs", "Devs", "miracledevs", "thirdparty", "Thirdparty", "unencrypted", "Unencrypted", "webapi"]
}
